/*This program will display a list of channels declared within an array */
#include <stdio.h>
#define NUMBERS 7
 
void display(int *);
 
int main()
{
     
int channels[NUMBERS]={2,4,5,7,9,11,13}; /*declares array*/
printf("Channels:");
display(channels);
}
void display(int *channels)
{
     int i;
     for(i=0;i<NUMBERS;i++)
     {
          printf("%5d",*(channels+i)); /*displays array*/
     }
     }
