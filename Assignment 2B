		printf("\nThe key entered is an alphabetic character", fkey);
		else if (fkey == 'm')
		printf("\nThe key entered is an alphabetic character", fkey);
		else if (fkey == 'n')
		printf("\nThe key entered is an alphabetic character", fkey);
		else if (fkey == 'o')
		printf("\nThe key entered is an alphabetic character", fkey);
		else if (fkey == 'p')
		printf("\nThe key entered is an alphabetic character", fkey);
		else if (fkey == 'q')
		printf("\nThe key entered is an alphabetic character", fkey);
		else if (fkey == 'r')
		printf("\nThe key entered is an alphabetic character", fkey);
		else if (fkey == 's')
		printf("\nThe key entered is an alphabetic character", fkey);
		else if (fkey == 't')
		printf("\nThe key entered is an alphabetic character", fkey);
		else if (fkey == 'u')
		printf("\nThe key entered is an alphabetic character", fkey);
		else if (fkey == 'v')
		printf("\nThe key entered is an alphabetic character", fkey);
		else if (fkey == 'w')
		printf("\nThe key entered is an alphabetic character", fkey);
		else if (fkey == 'x')
		printf("\nThe key entered is an alphabetic character", fkey);
		else if (fkey == 'y')
		printf("\nThe key entered is an alphabetic character", fkey);
		else if (fkey == 'z')
		printf("\nThe key entered is an alphabetic character", fkey);
			
			/*If key entered is A-Z it will show a result for alphabetic character*/
			else if (fkey == 'A')
			printf("\nThe key entered is an alphabetic character", fkey);
		else if (fkey == 'B')
		printf("\nThe key entered is an alphabetic character", fkey);
		else if (fkey == 'C')
		printf("\nThe key entered is an alphabetic character", fkey);
		else if (fkey == 'D')
		printf("\nThe key entered is an alphabetic character", fkey);
		else if (fkey == 'E')
		printf("\nThe key entered is an alphabetic character", fkey);
		else if (fkey == 'F')
		printf("\nThe key entered is an alphabetic character", fkey);
		else if (fkey == 'G')
		printf("\nThe key entered is an alphabetic character", fkey);
		else if (fkey == 'H')
		printf("\nThe key entered is an alphabetic character", fkey);
		else if (fkey == 'I')
		printf("\nThe key entered is an alphabetic character", fkey);
		else if (fkey == 'J')
		printf("\nThe key entered is an alphabetic character", fkey);
		else if (fkey == 'K')
		printf("\nThe key entered is an alphabetic character", fkey);
		else if (fkey == 'L')
		printf("\nThe key entered is an alphabetic character", fkey);
		else if (fkey == 'M')
		printf("\nThe key entered is an alphabetic character", fkey);
		else if (fkey == 'N')
		printf("\nThe key entered is an alphabetic character", fkey);
		else if (fkey == 'O')
		printf("\nThe key entered is an alphabetic character", fkey);
		else if (fkey == 'P')
		printf("\nThe key entered is an alphabetic character", fkey);
		else if (fkey == 'Q')
		printf("\nThe key entered is an alphabetic character", fkey);
