/*This program will allow the user to press a single key on the keyboard and then determine whether or 
not the character entered is an alphabetic character, digit, or special character*/

#include <stdio.h>
int main()
{
	char fkey; /*defines the character variable fkey*/
	printf("This program will define your single character input as either a digit,"
	"\nletter, or special character.\n\nPlease enter a single character to define: "); /*instructions for user*/
	scanf("%c", &fkey); /*allows user to input character for fkey*/
	
	/*If key entered is a-z it will show a result for alphabetic character*/
	if (fkey == 'a') 
		printf("\nThe key entered is an alphabetic character", fkey);
		else if (fkey == 'b')
			printf("\nThe key entered is an alphabetic character", fkey);
		else if (fkey == 'c')
		printf("\nThe key entered is an alphabetic character", fkey);
